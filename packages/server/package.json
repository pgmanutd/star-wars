{
  "name": "star-wars-server",
  "version": "1.0.0",
  "scripts": {
    "clean:build": "rm -rf build .netlify netlify-lambda",
    "start": "nodemon --exec babel-node -- ./src/index",
    "build": "cross-env NODE_ENV=production babel src --out-dir build && netlify-lambda build build/express",
    "serve": "node ./build"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.4",
    "awesome-graphql-utils": "^1.0.0",
    "axios": "^0.19.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^14.6.0",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "limiter": "^1.1.5",
    "lodash": "^4.17.15",
    "serverless-http": "^2.3.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-node-env-inline": "^0.4.3",
    "cross-env": "^7.0.0",
    "netlify-lambda": "^1.6.3",
    "nodemon": "^2.0.2"
  }
}
